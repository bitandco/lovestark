#[contract]
mod Love-Starknet {
    use starknet::get_caller_address;
    use starknet::ContractAddress;


    #[event]
    fn LoveStarknet(from: ContractAddress, value: felt252) {}


    #[external]
    fn Say_Love-Starknet(message: felt252) {
        let caller = get_caller_address();
        Hello(caller, message);
    }

}
